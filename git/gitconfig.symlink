[user]
    name = James Hanford
    email = james@projkt.io

[alias]
    blocal = branch
    bremote = branch -r
    bmerged = branch --merged

    # Checkout previous branch
    clast = checkout @{-1}

    log1 = log --oneline

    # Show the user email for the current repo
    whoami = config user.email

[branch]
    sort = -committerdate